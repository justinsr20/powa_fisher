<manifest>
    <main-class>com.thesmeg.bots.fleshcrawler.FleshCrawler</main-class>
    <name>Flesh Crawler Fighter</name>
    <tag-line>Kills Flesh Crawlers in Security Stronghold and banks!</tag-line><!--Max of 50 chars-->
    <description>Requires Varrock teleport skill/runes and food</description><!--Max of 110 chars-->
    <version>1.1.0</version>
    <compatibility>
        <game>OSRS</game>
    </compatibility>
    <categories>
        <category>Combat</category>
    </categories>
    <!--Required to publish on the bot store-->
    <internal-id>FleshCrawlerFighter</internal-id>
    <!--The rest are optional-->
    <open-source>false</open-source>
    <price>0.06</price>
    <hidden>false</hidden> <!--If you need to hide it from the bot store for maintenance-->
    <access>public</access>
    <tags>
        <tag>Combat, Pure, Flesh Crawler, Security Stronghold, Range</tag>
    </tags>
</manifest>