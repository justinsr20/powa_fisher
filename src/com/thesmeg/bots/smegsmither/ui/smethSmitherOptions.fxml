<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<TabPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
         prefWidth="600.0" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/8"
         xmlns:fx="http://javafx.com/fxml/1">
    <tabs>
        <Tab text="Status">
            <content>
                <AnchorPane prefHeight="200.0" prefWidth="200.0">
                    <children>
                        <TextField editable="false" layoutX="34.0" layoutY="30.0" prefHeight="25.0" prefWidth="171.0"
                                   text="Welcome to Smegs Smither"/>
                    </children>
                </AnchorPane>
            </content>
        </Tab>
        <Tab text="Smelting">
            <content>
                <AnchorPane prefHeight="200.0" prefWidth="200.0">
                    <children>
                        <ComboBox fx:id="comboBoxChooseBar" layoutX="38.0" layoutY="34.0" prefWidth="150.0"
                                  promptText="Pick bar to smelt" visibleRowCount="5"/>
                        <Button fx:id="btnSetConfig" layoutX="38.0" layoutY="293.0" mnemonicParsing="false"
                                text="Set Configuration"/>
                    </children>
                </AnchorPane>
            </content>
        </Tab>
    </tabs>
</TabPane>
