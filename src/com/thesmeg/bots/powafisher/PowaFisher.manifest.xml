<manifest>
    <main-class>com.thesmeg.bots.powafisher.PowaFisher</main-class>
    <name>thesmeg Free To Play Powerfisher</name>
    <tag-line>Powerfishes with a Small Net</tag-line><!--Max of 50 chars-->
    <description>Simply start bot with correct fishing equipment in inventory next to the correct fishing spot.</description><!--Max of 110 chars-->
    <version>1.0.0</version>
    <compatibility>
        <game>OSRS</game>
    </compatibility>
    <categories>
        <category>FISHING</category>
    </categories>
    <!--Required to publish on the bot store-->
    <internal-id>PowaFisher</internal-id>
    <!--The rest are optional-->
    <open-source>true</open-source>
    <hidden>false</hidden> <!--If you need to hide it from the bot store for maintenance-->
    <access>public</access>
    <tags>
        <tag>Fishing, Powerfishing</tag>
    </tags>
</manifest>